{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "TEXTCOLO",
    "patterns": [
        {
            "name": "variable.textcolo",
            "match": "\\#\\w+?\\b"
        },
        { "include": "source.mapinfolike#basicPatterns" },
        {
            "name": "keyword.control.textcolo",
            "match": "\\w+?\\b:"
        },
        {
            "name": "entity.name.class.textcolo",
            "match": "\\w+?"
        }
    ],
    "scopeName": "source.textcolo"
}
